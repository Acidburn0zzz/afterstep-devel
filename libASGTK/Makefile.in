LIB_INCS= asgtk.h asgtkapp.h asgtkai.h asgtkcolorsel.h asgtkdirtree.h asgtkgradient.h \
		asgtkimagebrowser.h asgtkimagedir.h asgtkimageview.h \
		asgtkxmleditor.h

LIB_OBJS= asgtkapp.o asgtkai.o asgtkcolorsel.o asgtkdirtree.o asgtkgradient.o \
		asgtkimagebrowser.o asgtkimagedir.o asgtkimageview.o \
		asgtkxmleditor.o

CONFIG_SCRIPT= asgtk-config

LIB_DIR_NAME    = libASGTK
LIB_NAME        = LIBASGTK
LIB_STATIC      = libASGTK.a
LIB_SHARED      = libASGTK.so
LIB_SHARED_CYG  = cygASGTK.dll
LIB_SHARED_CYG_AR  = libASGTK.dll.a
LIBVER          = @libasgtk_version@
LIBVERMAJOR     = @libasgtk_version_major@

INCS_PRIVATE	= @GTK_INCLUDES@

LIBS_PRIVATE      = ../src/afterstep/$(LIB_AFTERCONF) ../src/afterstep/$(LIB_AFTERSTEP) \
		    ../src/afterstep/$(LIB_AFTERIMAGE) ../src/afterstep/$(LIB_AFTERBASE) \
		    @GTK_LIBS@ $(LIBS_AFTERIMAGE)

@MAKEFILECOMMONLIB@



