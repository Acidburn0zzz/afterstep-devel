OBJS_ASCP       = ascp.o ascpui.o

OBJS_ASCOLOR    = ascolor.o

OBJS		= $(OBJS_ASCP) $(OBJS_ASCOLOR)

PROG		= @ASCP@ ascolor

INCS_PRIVATE	= @ASCP_INCLUDES@

LIBS_PRIVATE      = $(LIB_AFTERCONF) $(LIB_AFTERSTEP) \
					$(LIB_AFTERIMAGE) $(LIB_AFTERBASE) @ASCP_LIBS@ $(LIBS_AFTERIMAGE)
LIBS_DEPEND     = $(LIB_AFTERBASE) $(LIB_AFTERIMAGE) $(LIB_AFTERSTEP) $(LIB_AFTERCONF)

@MAKEFILECOMMON@


ascp:	        $(OBJS_ASCP) $(LIBS_DEPEND)
		$(CC) $(OBJS_ASCP) $(LIBS_ALL) -o $(@)

ascolor:        $(OBJS_ASCOLOR) $(LIBS_DEPEND)
		$(CC) $(OBJS_ASCOLOR) $(LIBS_ALL) -o $(@)


install:	install.bin install.man

uninstall:	uninstall.bin uninstall.man
